{
  "manifestVersion": 1,
  "id": "pr-pulse",
  "name": "PR Pulse",
  "version": "1.0.11",
  "publisher": "bacumi",
  "public": true,
  "tags": [
    "azure devops",
    "pull requests",
    "code review",
    "dashboard",
    "repos",
    "staleness alerts"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "The intelligent dashboard for all active pull requests, with built-in staleness alerts to keep your team moving.",
  "categories": [
    "Azure Repos"
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "scopes": [
    "vso.code",
    "vso.settings"
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    },
    "changelog": {
      "path": "CHANGELOG.md"
    }
  },
  "links": {
        "getstarted": {
            "uri": "https://github.com/mihaibc/pr_pulse"
        },
        "support": {
            "uri": "mailto:contact@bacumi.com"
        }
    },
  "repository": {
        "type": "git",
        "uri": "https://github.com/mihaibc/pr_pulse"
    },
  "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "main.js",
      "addressable": true
    },
    {
      "path": "helpers.js",
      "addressable": true
    },
    {
      "path": "lib",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "CHANGELOG.md",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "pr-pulse-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "name": "PR Pulse",
        "icon": "images/hub-icon.png",
        "uri": "index.html",
        "order": 35
      }
    }
  ]
}
